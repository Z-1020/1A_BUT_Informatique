<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="iut.gon.test1.SecondaryController">
    <children>
      <ToolBar prefHeight="57.0" prefWidth="399.0">
         <items>
            <ToggleButton mnemonicParsing="false" selected="true" text="ToggleButton" />
            <ToggleButton mnemonicParsing="false" text="ToggleButton">
               <toggleGroup>
                  <ToggleGroup fx:id="groupe" />
               </toggleGroup>
            </ToggleButton>
            <ToggleButton mnemonicParsing="false" text="ToggleButton" toggleGroup="$groupe" />
            <ToggleButton mnemonicParsing="false" text="ToggleButton" toggleGroup="$groupe" />
         </items>
      </ToolBar>
        <Label text="Secondary View" />
        <Button fx:id="secondaryButton" onAction="#switchToPrimary" text="Switch to Primary View" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
